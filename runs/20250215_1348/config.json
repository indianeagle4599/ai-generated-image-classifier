{
    "data": {
        "data_path": "dataset",
        "train_csv": "augmented_20250214_2324.csv",
        "test_csv": "test.csv",
        "batch_size": 256,
        "num_workers": 4,
        "image_size": [
            224,
            224
        ],
        "num_classes": 2
    },
    "model": {
        "model_name": "vit_b_16",
        "pretrained": true
    },
    "training": {
        "epochs": 25,
        "learning_rate": 0.001,
        "momentum": 0.9,
        "weight_decay": 0.0001
    },
    "augmentation": {
        "use_augmentation": false,
        "options": {
            "random_crop": true,
            "horizontal_flip": true,
            "color_jitter": true,
            "rotation": 10
        }
    }
}